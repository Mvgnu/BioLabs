+from app.main import app
+from app.auth import get_current_user
+
+PUBLIC_PATHS = {
+    "/api/auth/login",
+    "/api/auth/register",
+    "/api/auth/request-password-reset",
+    "/api/auth/reset-password",
+    "/metrics",
+    "/api/marketplace/listings",
+    "/api/services/listings",
+    "/api/protocols/public",
+}
