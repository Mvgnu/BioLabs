+        raise HTTPException(status_code=404, detail="Location not found")
+    if loc.team_id:
+        check_team_role(db, user, loc.team_id, ["owner", "manager"])
+    for k, v in update.model_dump(exclude_unset=True).items():
+        setattr(loc, k, v)
+    db.commit()
+    db.refresh(loc)
+    return loc
+
